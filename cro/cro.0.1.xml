    <!-- https://www.openriskmanual.org/ns/cro/BusinessModel -->


    <owl:Class rdf:about="https://www.openriskmanual.org/ns/cro/BusinessModel">
        <rdfs:comment>The core (rating specific) business model choices adopted by a rating agency</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.openriskmanual.org/ns/cro.owl"/>
        <rdfs:label>BusinessModel</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://www.openriskmanual.org/wiki/Credit_Rating_Agency"/>
    </owl:Class>


    <!-- https://www.openriskmanual.org/ns/cro/CreditRating -->


    <owl:Class rdf:about="https://www.openriskmanual.org/ns/cro/CreditRating">
        <rdfs:comment>The core credit rating class encapsulating in particular all data elements required to parse
            credit data
        </rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.openriskmanual.org/ns/cro.owl"/>
        <rdfs:label>CreditRating</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://www.openriskmanual.org/wiki/Credit_Rating"/>
    </owl:Class>


    <!-- https://www.openriskmanual.org/ns/cro/CreditRatingAgency -->


    <owl:Class rdf:about="https://www.openriskmanual.org/ns/cro/CreditRatingAgency">
        <rdfs:comment>The core Credit Rating Agency Class representing an entity issuing credit ratings</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.openriskmanual.org/ns/cro.owl"/>
        <rdfs:label>CreditRatingAgency</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://www.openriskmanual.org/wiki/Credit_Rating_Agency"/>
    </owl:Class>


    <!-- https://www.openriskmanual.org/ns/cro/CreditRatingMethodology -->


    <owl:Class rdf:about="https://www.openriskmanual.org/ns/cro/CreditRatingMethodology">
        <rdfs:comment>The credit rating methodology adopted by a rating agency (can be multiple)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.openriskmanual.org/ns/cro.owl"/>
        <rdfs:label>CreditRatingMethodology</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://www.openriskmanual.org/ns/cro/Credit%20Rating%20Methodology"/>
    </owl:Class>


    <!-- https://www.openriskmanual.org/ns/cro/CreditRatingModel -->


    <owl:Class rdf:about="https://www.openriskmanual.org/ns/cro/CreditRatingModel">
        <rdfs:comment>A concrete credit rating model (defined analytical process, including possible qualitative
            aspects)
        </rdfs:comment>
        <rdfs:comment>In future revisions credit risk models will be linked with the risk model ontology</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.openriskmanual.org/ns/cro.owl"/>
        <rdfs:label>CreditRatingModel</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://www.openriskmanual.org/wiki/Credit_Rating_Model"/>
    </owl:Class>


    <!-- https://www.openriskmanual.org/ns/cro/CreditRatingScale -->


    <owl:Class rdf:about="https://www.openriskmanual.org/ns/cro/CreditRatingScale">
        <rdfs:comment>The set of labels, grades over which a credit rating may range</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.openriskmanual.org/ns/cro.owl"/>
        <rdfs:label>CreditRatingScale</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://www.openriskmanual.org/wiki/Credit_Rating_Scale"/>
    </owl:Class>


