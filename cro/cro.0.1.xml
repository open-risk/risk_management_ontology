    </owl:DatatypeProperty>


    <!-- https://www.openriskmanual.org/ns/cro/has_default_definition -->


    <owl:DatatypeProperty rdf:about="https://www.openriskmanual.org/ns/cro/has_default_definition">
        <rdfs:domain rdf:resource="https://www.openriskmanual.org/ns/cro/CreditRatingMethodology"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The default definition as disclosed</rdfs:comment>
        <rdfs:seeAlso rdf:resource="https://www.openriskmanual.org/wiki/Default_Definition"/>
    </owl:DatatypeProperty>


    <!-- https://www.openriskmanual.org/ns/cro/has_information_model -->


    <owl:DatatypeProperty rdf:about="https://www.openriskmanual.org/ns/cro/has_information_model">
        <rdfs:domain rdf:resource="https://www.openriskmanual.org/ns/cro/BusinessModel"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>Public and Private</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>Public only</rdf:first>
                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>Whether the agency uses private information in the formation of ratings</rdfs:comment>
    </owl:DatatypeProperty>


    <!-- https://www.openriskmanual.org/ns/cro/has_legal_entity_type -->


