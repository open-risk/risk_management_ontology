        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#Name"/>
        <rdfs:comment>Agency full name</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://www.openriskmanual.org/ns/cro/has_origination_type -->


    <owl:DatatypeProperty rdf:about="https://www.openriskmanual.org/ns/cro/has_origination_type">
        <rdfs:domain rdf:resource="https://www.openriskmanual.org/ns/cro/CreditRating"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
