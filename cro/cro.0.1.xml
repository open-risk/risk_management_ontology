    <!-- https://www.openriskmanual.org/ns/cro/has_profit_profile -->


    <owl:DatatypeProperty rdf:about="https://www.openriskmanual.org/ns/cro/has_profit_profile">
        <rdfs:domain rdf:resource="https://www.openriskmanual.org/ns/cro/BusinessModel"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>For-profit</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>Non-profit</rdf:first>
                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                            </rdf:Description>
