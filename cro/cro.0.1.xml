    <!-- https://www.openriskmanual.org/ns/cro/BusinessModel -->


    <owl:Class rdf:about="https://www.openriskmanual.org/ns/cro/BusinessModel">
        <rdfs:comment>The core (rating specific) business model choices adopted by a rating agency</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.openriskmanual.org/ns/cro.owl"/>
        <rdfs:label>BusinessModel</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://www.openriskmanual.org/wiki/Credit_Rating_Agency"/>
    </owl:Class>
    


    <!-- https://www.openriskmanual.org/ns/cro/CreditRating -->


    <owl:Class rdf:about="https://www.openriskmanual.org/ns/cro/CreditRating">
        <rdfs:comment>The core credit rating class encapsulating in particular all data elements required to parse credit data</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.openriskmanual.org/ns/cro.owl"/>
        <rdfs:label>CreditRating</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://www.openriskmanual.org/wiki/Credit_Rating"/>
    </owl:Class>
    


    <!-- https://www.openriskmanual.org/ns/cro/CreditRatingAgency -->


    <owl:Class rdf:about="https://www.openriskmanual.org/ns/cro/CreditRatingAgency">
