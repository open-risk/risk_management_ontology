                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>Relative</rdf:first>
                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>The absolute or relative (to peers) nature of a credit rating</rdfs:comment>
    </owl:DatatypeProperty>


    <!-- https://www.openriskmanual.org/ns/cro/has_risk_horizon -->


    <owl:DatatypeProperty rdf:about="https://www.openriskmanual.org/ns/cro/has_risk_horizon">
        <rdfs:domain rdf:resource="https://www.openriskmanual.org/ns/cro/CreditRating"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>Long Term</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>Short Term</rdf:first>
                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>The temporal risk horizon that is targeted by the credit rating</rdfs:comment>
    </owl:DatatypeProperty>


    <!-- https://www.openriskmanual.org/ns/cro/has_solicitation_policy -->


    <owl:DatatypeProperty rdf:about="https://www.openriskmanual.org/ns/cro/has_solicitation_policy">
        <rdfs:domain rdf:resource="https://www.openriskmanual.org/ns/cro/BusinessModel"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>Solicited and Unsolicited</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>Solicited only</rdf:first>
                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>Whether the agency business model includes unsolicited (by the borrower) ratings</rdfs:comment>
    </owl:DatatypeProperty>


    <!-- https://www.openriskmanual.org/ns/cro/has_subsidiary_policy -->


    <owl:DatatypeProperty rdf:about="https://www.openriskmanual.org/ns/cro/has_subsidiary_policy">
        <rdfs:domain rdf:resource="https://www.openriskmanual.org/ns/cro/CreditRatingMethodology"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>


    <!-- https://www.openriskmanual.org/ns/cro/has_temporal_type -->


    <owl:DatatypeProperty rdf:about="https://www.openriskmanual.org/ns/cro/has_temporal_type">
        <rdfs:domain rdf:resource="https://www.openriskmanual.org/ns/cro/CreditRating"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
