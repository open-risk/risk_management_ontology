        <rdfs:domain rdf:resource="https://www.openriskmanual.org/ns/cro/CreditRating"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>Instrument Rating</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>Issuer Rating</rdf:first>
                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>Whether the rating concerns an issuer (legal entity) or an instrument (debt security)
        </rdfs:comment>
    </owl:DatatypeProperty>


    <!-- https://www.openriskmanual.org/ns/cro/has_methodology_description -->


    <owl:DatatypeProperty rdf:about="https://www.openriskmanual.org/ns/cro/has_methodology_description">
