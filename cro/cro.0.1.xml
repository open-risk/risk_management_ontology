        <rdfs:isDefinedBy rdf:resource="https://www.openriskmanual.org/ns/cro.owl"/>
        <rdfs:label>CreditRatingAgency</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://www.openriskmanual.org/wiki/Credit_Rating_Agency"/>
    </owl:Class>
    


    <!-- https://www.openriskmanual.org/ns/cro/CreditRatingMethodology -->


    <owl:Class rdf:about="https://www.openriskmanual.org/ns/cro/CreditRatingMethodology">
